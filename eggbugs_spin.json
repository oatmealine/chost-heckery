{"modules":[{"id":"1332d0e28b518270","data":{"contents":"<script>\n  import Eggbug from './Eggbug.svelte';\n  import grad from './grad';\n  \n  function entries(i) {\n    return Array(i).fill(0).map((v,i) => i);\n  }\n\n  function off(ang, pow) {\n    return `transform:translate(${Math.cos(ang/360*Math.PI*2) * pow}px,${Math.sin(ang/360*Math.PI*2) * pow}px)`;\n  }\n</script>\n\n<style>\n  .eggbug-container {\n    position: relative;\n    width: 96px;\n    height: 96px;\n  }\n  .eggbug, .eggbugs {\n    position: absolute;\n  }\n  .outer {\n    display: flex;\n    width: 100%;\n    height: 300px;\n    align-items: center;\n    justify-content: center;\n  }\n  .spin {\n    animation: 10s spin infinite linear;\n  }\n  .unspin {\n    animation: 10s spin infinite linear;\n    animation-direction: reverse;\n  }\n  .scale {\n    width: 96px;\n    height: 96px;\n  }\n  .spin-step {\n    animation: 10s spin infinite linear;\n    animation-timing-function: steps(16);\n  }\n  .unspin-step {\n    animation: 10s spin infinite linear;\n    animation-timing-function: steps(16);\n    animation-direction: reverse;\n    animation-delay: 0.625s;\n  }\n  .fade {\n    width: 96px;\n    height: 96px;\n    position: absolute;\n    z-index: 1;\n  }\n  .fade-grad {\n    height: 96px;\n    width: 2048px;\n    background-size: 2048px 96px;\n    animation: 0.625s spin infinite linear;\n    transform: translateX(-1952px);\n  }\n  .faded {\n    position: absolute;\n    z-index: 0;\n  }\n  .pulse {\n    animation: 2.5s spin infinite linear;\n    transform: scale(1.3, 1.3);\n    animation-direction: alternate-reverse;\n    animation-timing-function: ease-in-out;\n  }\n  .fade-parent {\n    overflow: hidden;\n  }\n</style>\n\n<div class=\"outer\">\n  <div class=\"eggbug-container\">\n    <div class=\"scale spin-step eggbugs\">\n      {#each entries(6) as i}\n      <div class=\"eggbug\">\n        <div style=\"{off(i/6*360, 120)}\">\n          <div class=\"scale unspin-step fade-parent\">\n            <div class=\"faded scale\"><Eggbug color={i + 1}/></div>\n            <div class=\"fade\"><div class=\"fade-grad\" style=\"background-image: url('{grad}')\"></div></div>\n          </div>\n        </div>\n      </div>\n      {/each}\n    </div>\n    <div class=\"scale spin eggbugs\">\n      <div class=\"eggbug unspin\"><div class=\"pulse\"><Eggbug color={0}/></div></div>\n      {#each entries(6) as i}\n      <div class=\"eggbug\">\n        <div style=\"{off(i/6*360, 120)}\">\n          <div class=\"unspin\">\n            <Eggbug color={i + 1}/>\n          </div>\n        </div>\n      </div>\n      {/each}\n    </div>\n  </div>\n</div>"},"pluginId":"source.svelte","sends":["7884bebfbe7298bc"],"namedSends":{}},{"id":"cc7b0c9c74f38b04","data":{"name":"Eggbug","contents":"<script>\n  const eggbugs = [\n    'https://cdn.discordapp.com/attachments/961685563583373362/1041466685623894127/eggbug-gray.png',\n    'https://cdn.discordapp.com/attachments/961685563583373362/1041466685925892196/eggbug-red.png',\n    'https://cdn.discordapp.com/attachments/961685563583373362/1041466684524998656/eggbug-orange.png',\n    'https://cdn.discordapp.com/attachments/961685563583373362/1041466684214616206/eggbug-yellow.png',\n    'https://cdn.discordapp.com/attachments/961685563583373362/1041466683908423820/eggbug-green.png',\n    'https://cdn.discordapp.com/attachments/961685563583373362/1041466684843753594/eggbug-blue.png',\n    'https://cdn.discordapp.com/attachments/961685563583373362/1041466685242220584/eggbug-etal.png'\n  ];\n\n  export let color;\n</script>\n\n<style>\n  img {\n    margin: 0;\n    height: 96px;\n    width: 96px;\n  }\n</style>\n\n<img src=\"{eggbugs[color]}\" width=\"96\" height=\"96\">"},"pluginId":"source.svelte-component","sends":[],"namedSends":{"1332d0e28b518270":["Eggbug"]}},{"id":"7884bebfbe7298bc","data":{"mode":"attr"},"pluginId":"transform.style-inliner","sends":["output"],"namedSends":{}},{"id":"05ffbe6408972f09","data":{"contents":"<svg width=\"96\" height=\"96\" version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\">\n  <defs>\n    <linearGradient id=\"g\">\n      <stop offset=\"0%\" stop-color=\"rgba(255,255,255,1)\" />\n      <stop offset=\"100%\" stop-color=\"rgba(255,255,255,0)\" />\n    </linearGradient>\n  </defs>\n  <rect x=\"0\" y=\"0\" width=\"96\" height=\"96\" style=\"fill:url(#g)\"/>\n</svg>","language":"html"},"pluginId":"source.text","sends":["98d555e4c4257b85"],"namedSends":{}},{"id":"98d555e4c4257b85","data":{},"pluginId":"transform.svgo","sends":["afe0b8b89c40e41d"],"namedSends":{}},{"id":"afe0b8b89c40e41d","data":{"mime":"image/svg+xml"},"pluginId":"transform.to-data-url","sends":[],"namedSends":{"1332d0e28b518270":["grad"]}},{"id":"90ebdbc26f7bdf93","data":{"contents":"","language":"text"},"pluginId":"source.text","sends":[],"namedSends":{}}]}