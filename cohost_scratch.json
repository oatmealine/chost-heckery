{"modules":[{"id":"87736d8caf60a3f5","data":{"contents":"<style>\n  .player-wrapper {\n    width: 360px;\n    height: 400px;\n    pointer-events: none !important;\n    position: relative;\n  }\n  .underlay-wrapper {\n    position: relative;\n  }\n  .underlay {\n    position: absolute;\n    inset: 0;\n  }\n\n  .button-row {\n    height: 2.75rem;\n    padding-top: 0.5rem;\n    padding-bottom: 0.5rem;\n\n    display: flex;\n    flex-direction: row;\n  }\n\n  .button {\n    width: 2rem;\n    height: 2rem;\n    padding: 0.375rem;\n    border-radius: 0.25rem;\n    -webkit-user-select: none;\n    -moz-user-select: none;\n    -ms-user-select: none;\n    user-select: none;\n    user-drag: none;\n    cursor: pointer;\n    margin-top: 0 !important;\n    margin-bottom: 0 !important;\n  }\n  .button.active {\n    background-color: hsla(215, 100%, 65%, 0.35);\n  }\n\n  .no-interact {\n    pointer-events: none;\n  }\n\n  .hidden {\n    position: absolute;\n    inset: 0;\n  }\n  .shown {\n    position: absolute;\n    left: 0;\n    top: 0;\n  }\n  .shown {\n    font-size: 0;\n  }\n  .hidden {\n    pointer-events: none;\n  }\n  .yes-interact {\n    pointer-events: auto !important;\n  }\n</style>\n\n<script>\n  import Player from './Player.svelte';\n  import GreenFlag from './GreenFlag';\n  import StopAll from './StopAll';\n  import Editor from './Editor.svelte';\n</script>\n\n<div class=\"underlay-wrapper\">\n  <div class=\"underlay\">\n    <div class=\"button-row\">\n      <img class=\"button green-flag no-interact\" draggable=\"false\" src={GreenFlag} tile=\"Go\">\n      <img class=\"button stop-all\" draggable=\"false\" src={StopAll} title=\"Stop\">\n    </div>\n    <Player active={false}/>\n  </div>\n</div>\n<details class=\"player-wrapper\">\n  <summary class=\"shown\">\n    <div class=\"button-row\">\n      <img class=\"button green-flag yes-interact\" draggable=\"false\" src={GreenFlag} tile=\"Go\">\n    </div>\n  </summary>\n  <div class=\"hidden\">\n    <div class=\"button-row\">\n      <img class=\"button green-flag active yes-interact\" draggable=\"false\" src={GreenFlag} tile=\"Go\">\n      <img class=\"button stop-all\" draggable=\"false\" src={StopAll} title=\"Stop\">\n    </div>\n    <div class=\"yes-interact\">\n      <Player active={true}/>\n    </div>\n  </div>\n</details>\n\n<p>\n  am i doing the whole \"css crimes\" thing right?\n</p>\n\n<details>\n  <summary>\n    source code (in scratch, of course)\n  </summary>\n  <Editor/>\n</details>"},"pluginId":"source.svelte","sends":["a13ec312d06590d7"],"namedSends":{}},{"id":"ad3f73042d552d84","data":{"name":"Player","contents":"<script>\n  export let active = false;\n  import ScratchCat from './ScratchCat';\n  import ScratchCatStill from './ScratchCatStill';\n</script>\n\n<style>\n  .player {\n    height: 360px;\n    width: 480px;\n    border-radius: 0.5rem;\n    border: 0.0625rem solid hsla(0, 0%, 0%, 0.15);\n    overflow: hidden;\n    background-color: #fff;\n    -ms-touch-action: none;\n    touch-action: none;\n    -webkit-box-sizing: content-box !important;\n    box-sizing: content-box !important;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n  }\n</style>\n\n<div class=\"player\">\n  {#if !active}\n    <img src={ScratchCatStill} width=\"128px\" height=\"auto\"/>\n  {:else}\n    <div style=\"transform: translate(0px, 64px)\">\n      <div style=\"animation: spin 6s steps(8) infinite\">\n        <img src={ScratchCat} width=\"128px\" height=\"auto\" style=\"transform: translate(0px, -64px)\"/>\n      </div>\n    </div>\n  {/if}\n</div>"},"pluginId":"source.svelte-component","sends":["87736d8caf60a3f5"],"namedSends":{}},{"id":"a13ec312d06590d7","data":{"mode":"attr"},"pluginId":"transform.style-inliner","sends":["output"],"namedSends":{}},{"id":"61d035b30eb508d8","data":{"contents":"https://staging.cohostcdn.org/attachment/2a79b9c0-4a3c-4f5c-b908-fd9e2db587cb/icon--green-flag.svg","language":"text"},"pluginId":"source.text","sends":[],"namedSends":{"